<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping auto-import="false">
  <class name="org.ifmc.mat.reportmodel.Metadata" table="METADATA" lazy="false">
	<id name="id" column="METADATA_ID">
		<generator class="uuid"/>
	</id>
	<property name="name" column="NAME" />
	<property name="value" column="VALUE" />

	<many-to-one name="measure" column="MEASURE_ID" class="org.ifmc.mat.reportmodel.Measure" unique="true" lazy="false" cascade="all" />
					
  </class>
</hibernate-mapping>
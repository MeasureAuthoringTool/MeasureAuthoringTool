<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping auto-import="false">
  <class name="mat.reportmodel.QualityDataSet" table="QUALITY_DATA_MODEL" lazy="false">
	<id name="id" column="QUALITY_DATA_MODEL_ID" >
		<generator class="uuid"/>
	</id>
	
	<many-to-one name="dataType" column="DATA_TYPE_ID" 
		class="mat.reportmodel.DataType" unique="false" lazy="false" cascade="none" />
	<many-to-one name="listObject" column="LIST_OBJECT_ID" 
		class="mat.reportmodel.ListObject" unique="false" lazy="false" cascade="none" />
	<many-to-one name="measureId" column="MEASURE_ID" 
		class="mat.reportmodel.Measure" unique="false" lazy="false" cascade="none" />
	<property name="version" column="VERSION" />	
	<property name="oid" column="OID" />
	<property name="occurrence" column="OCCURRENCE" />
  </class>
</hibernate-mapping>
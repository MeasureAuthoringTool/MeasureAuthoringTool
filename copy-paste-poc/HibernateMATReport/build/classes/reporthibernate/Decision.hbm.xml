<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping auto-import="false">
  <class name="mat.reportmodel.Decision" table="DECISION" lazy="false">
	<id name="id" column="ID" >
	</id>
	<property name="parentId" column="PARENT_ID" />	
	<property name="operator" column="OPERATOR" />
	<property name="orderNum" column="ORDER_NUM" />
	<property name="clauseId" column="CLAUSE_ID" />      
	<property name="attributeId" column="ATTRIBUTE_ID" /> 
  </class>
</hibernate-mapping>
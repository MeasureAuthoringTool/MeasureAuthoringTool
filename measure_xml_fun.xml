        <definitions>
            <definition context="Patient"
                        name="Antibiotic Active Overlaps Episode and Starts More than 30 Days Prior to Episode"
                        popDefinition="false" supplDataElement="false">
                <logic>"Episode Date" EpisodeDate
                    with ["MedicationStatement": "CWP Antibiotic Medications"] ActiveAntibiotic
                    such that ActiveAntibiotic.effectiveDateTime starts 31 days or more before day of start of
                    EpisodeDate.relevantPeriod
                    and ActiveAntibiotic.effectiveDateTime overlaps day of EpisodeDate.relevantPeriod
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" name="Antibiotic Dispensed During 30 Days Prior" popDefinition="false"
                        supplDataElement="false">
                <logic>"Episode Date" EpisodeDate
                    with ["MedicationDispense": "CWP Antibiotic Medications"] DispensedAntibiotic
                    such that DispensedAntibiotic.whenHandedOver during day of Interval[( start of
                    EpisodeDate.relevantPeriod - 30 days ), ( start of EpisodeDate.relevantPeriod - 1 day )]
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" name="Comorbid Condition History" popDefinition="false"
                        supplDataElement="false">
                <logic>"Episode Date" EpisodeDate
                    with ( ["Condition": "HIV"]
                    union ["Condition": "HIV Type 2"]
                    union ["Condition": "Malignant Neoplasms"]
                    union ["Condition": "Other Malignant Neoplasm of Skin"]
                    union ["Condition": "Emphysema"]
                    union ["Condition": "COPD"]
                    union ["Condition": "Disorders of the Immune System"]
                    union ["Condition": "Comorbid Conditions"] ) ComorbidCondition
                    such that ComorbidCondition.onsetDateTime starts during day of Interval[( start of
                    EpisodeDate.relevantPeriod - 12 months ), ( start of EpisodeDate.relevantPeriod )]
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" name="Competing Diagnosis History" popDefinition="false"
                        supplDataElement="false">
                <logic>"Episode Date" EpisodeDate
                    with ["Condition": "Competing Diagnosis"] CompetingCondition
                    such that start of CompetingCondition.onsetDateTime during day of Interval[( start of
                    EpisodeDate.relevantPeriod ), ( start of EpisodeDate.relevantPeriod + 3 days )]
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" name="Deduplicated Episode" popDefinition="false" supplDataElement="false">
                <logic>( { "First Episode", "Second Episode", "Third Episode", "Fourth Episode", "Fifth Episode", "Sixth
                    Episode", "Seventh Episode", "Eighth Episode", "Ninth Episode", "Tenth Episode", "Eleventh Episode",
                    "Twelfth Episode" } ) Episode
                    where Episode is not null
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" name="Denominator" popDefinition="false" supplDataElement="false">
                <logic>"Initial Population"</logic>
                <comment/>
            </definition>
            <definition context="Patient" name="Eighth Episode" popDefinition="false" supplDataElement="false">
                <logic>First("Episode Without Exclusions" WithoutExclusions
                    where start of WithoutExclusions.relevantPeriod 31 days or more on or after day of start of "Seventh
                    Episode".relevantPeriod
                    sort by start of relevantPeriod
                    )
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" name="Eleventh Episode" popDefinition="false" supplDataElement="false">
                <logic>First("Episode Without Exclusions" WithoutExclusions
                    where start of WithoutExclusions.relevantPeriod 31 days or more on or after day of start of "Tenth
                    Episode".relevantPeriod
                    sort by start of relevantPeriod
                    )
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" name="Encounter with Pharyngitis" popDefinition="false"
                        supplDataElement="false">
                <logic>( ( ["Encounter": "Outpatient"]
                    union ["Encounter": "Observation"]
                    union ["Encounter": "ED"]
                    union ["Encounter": "Telephone Visits"]
                    union ["Encounter": "Online Assessments"] ) Encounter
                    where exists ( Encounter.Unknown Dx
                    where Dx.code in "Pharyngitis"
                    ) ) PharynEncounter
                    where PharynEncounter.period starts during "Intake Period"
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" name="Encounter with Pharyngitis and Antibiotic" popDefinition="false"
                        supplDataElement="false">
                <logic>"Encounter with Pharyngitis" AllowablePharynEncounter
                    with ["MedicationDispense": "CWP Antibiotic Medications"] DispensedAntibiotic
                    such that DispensedAntibiotic.whenHandedOver during day of Interval[( start of
                    AllowablePharynEncounter.relevantPeriod ), ( start of AllowablePharynEncounter.relevantPeriod + 3
                    days )]
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" name="Episode Date" popDefinition="false" supplDataElement="false">
                <logic>// Participation logic is included in the 'Episode Date' definition
                    "Encounter with Pharyngitis and Antibiotic" PharynAntibioticEncounter
                    with ["Patient": "Birth date"] BirthDate
                    such that Common."CalendarAgeInYearsAt" ( BirthDate.birthDate, start of
                    PharynAntibioticEncounter.relevantPeriod ) &gt;= 3
                    where ( Common."Is Enrolled" ( "Product Line",
                    end of PharynAntibioticEncounter.relevantPeriod, Interval[start of
                    PharynAntibioticEncounter.relevantPeriod - 30 days,
                    end of PharynAntibioticEncounter.relevantPeriod + 3 days], 0 )
                    )
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" name="Episode Date with Hospice Intervention or Encounter"
                        popDefinition="false" supplDataElement="false">
                <logic>"Initial Population" IP
                    where exists Common."Hospice Intervention or Encounter"
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" name="Episode Without Exclusions" popDefinition="false"
                        supplDataElement="false">
                <logic>"Episode Date"
                    except ( "Outpatient ED or Observation Visit Resulting in Inpatient Stay"
                    union "Comorbid Condition History"
                    union "Medication History"
                    union "Competing Diagnosis History"
                    )
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" name="Exclusions" popDefinition="false" supplDataElement="false">
                <logic>"Episode Date with Hospice Intervention or Encounter"</logic>
                <comment/>
            </definition>
            <definition context="Patient" name="Fifth Episode" popDefinition="false" supplDataElement="false">
                <logic>First("Episode Without Exclusions" WithoutExclusions
                    where start of WithoutExclusions.relevantPeriod 31 days or more on or after day of start of "Fourth
                    Episode".relevantPeriod
                    sort by start of relevantPeriod
                    )
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" name="First Episode" popDefinition="false" supplDataElement="false">
                <logic>First("Episode Without Exclusions" WithoutExclusions
                    sort by start of relevantPeriod
                    )
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" name="Fourth Episode" popDefinition="false" supplDataElement="false">
                <logic>First("Episode Without Exclusions" WithoutExclusions
                    where start of WithoutExclusions.relevantPeriod 31 days or more on or after day of start of "Third
                    Episode".relevantPeriod
                    sort by start of relevantPeriod
                    )
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" name="Initial Population" popDefinition="false" supplDataElement="false">
                <logic>"Deduplicated Episode"</logic>
                <comment/>
            </definition>
            <definition context="Patient" name="Intake Period" popDefinition="false" supplDataElement="false">
                <logic>Interval[( start of "Measurement Period" - 6 months ), ( start of "Measurement Period" + 6 months
                    ) )
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" name="Medication History" popDefinition="false" supplDataElement="false">
                <logic>"Antibiotic Dispensed During 30 Days Prior"
                    union "Antibiotic Active Overlaps Episode and Starts More than 30 Days Prior to Episode"
                </logic>
                <comment/>
            </definition>
            <definition context="Patient" name="Ninth Episode" popDefinition="false" supplDataElement="false">
                <logic>First("Episode Without Exclusions" WithoutExclusions

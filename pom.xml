            -->
            <id>gwt-profile-compile-dev</id>
            <activation>
                <property>
                    <name>env.ENVIRONMENT</name>
                    <value>DEV</value>
                </property>
            </activation>
            <properties>
                <gwt.draftCompile>true</gwt.draftCompile>
                <gwt.localWorkers>10</gwt.localWorkers>
                <!-- Enable for detailed compilation traces. It doesn't affect compilation time. -->
<!--                <gwt.logLevel>TRACE</gwt.logLevel>-->
                <gwt.optimize>0</gwt.optimize>
                <gwt.style>DETAILED</gwt.style>
            </properties>
            <dependencies>
                <!-- Override dependencies to that you can locally run GWT code server with Jetty -->
                <dependency>
                    <groupId>com.google.gwt</groupId>
                    <artifactId>gwt-dev</artifactId>
                    <version>${gwtVersion}</version>
                    <scope>compile</scope>
                    <exclusions>
                        <!-- Should be empty to be merged correctly -->
                    </exclusions>
                </dependency>
            </dependencies>
        </profile>
        <profile>
            <id>install-libs</id>
            <build>
                <plugins>
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-install-plugin</artifactId>
                        <version>3.0.0-M1</version>
                        <executions>
                            <execution>
                                <phase>validate</phase>
                                <id>install-CQLtoELM</id>
                                <goals>
                                    <goal>install-file</goal>
                                </goals>
                                <configuration>
                                    <file>${project.basedir}/lib/CQLtoELM-1.4.6.54.jar</file>
                                    <groupId>mat</groupId>
                                    <artifactId>CQLtoELM</artifactId>
                                    <version>1.4.6.54</version>
                                    <packaging>jar</packaging>
                                    <localRepositoryPath>${maven.project.repo.dir}</localRepositoryPath>
                                </configuration>
                            </execution>
                            <execution>
                                <phase>validate</phase>
                                <id>install-vsac</id>
                                <goals>
                                    <goal>install-file</goal>
                                </goals>
                                <configuration>
                                    <file>${project.basedir}/lib/vsac-1.0.jar</file>
                                    <groupId>mat</groupId>
                                    <artifactId>vsac</artifactId>
                                    <version>1.0</version>
                                    <packaging>jar</packaging>
                                    <localRepositoryPath>${project.local.repo.dir}</localRepositoryPath>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
        </profile>
    </profiles>
</project>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
                         " http://castor.codehaus.org/mapping.dtd ">
<mapping>
   <class name="mat.model.VSACExpansionIdentifierWrapper">
		<map-to xml="ProfileList" />
		
		<field name="expIdentifierList" type="mat.model.VSACExpansionIdentifier"
		get-method="getExpIdentifierList" set-method="setExpIdentifierList"
           collection="arraylist">
      <bind-xml name="profile">
      	<class name="mat.model.VSACExpansionIdentifier">    
		    <field name="name" type="java.lang.String">
		      <bind-xml  node="text"/>
		    </field>
  		</class>
      </bind-xml>
    </field>
	</class>
</mapping>                         
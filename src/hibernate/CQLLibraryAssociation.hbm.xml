<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.org/dtd/hibernate-mapping.dtd">
	
<hibernate-mapping auto-import="false">
  <class name="mat.model.cql.CQLLibraryAssociation" table="CQL_LIBRARY_ASSOCIATION" lazy="true">
    <id name="id" column="ID" >
        <generator class="uuid"/>
    </id>
    <property name="associationId" column="ASSOCIATION_ID"/>
	<property name="cqlLibraryId" column="CQL_LIBRARY_ID" />
  </class>
</hibernate-mapping>

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.org/dtd/hibernate-mapping.dtd">
	
<hibernate-mapping auto-import="false">
  <class name="mat.model.CompositeMeasures" table="COMPOSITE_MEASURES" lazy="false">
	<id name="id" column="ID" >
		<generator class="increment"/>
	</id>	
	<property name="compositeMeasureId" column="COMPOSITE_MEASURE_ID" />
	<property name="componentMeasureId" column="COMPONENT_MEASURE_ID" />
	<property name="alias" column="ALIAS" />
  </class>
</hibernate-mapping>
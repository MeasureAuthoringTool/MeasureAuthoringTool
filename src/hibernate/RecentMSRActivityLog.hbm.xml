<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.org/dtd/hibernate-mapping.dtd">
	
<hibernate-mapping auto-import="false">
  <class name="mat.model.RecentMSRActivityLog" table="RECENT_MSR_ACTIVITY_LOG" >
	<id name="id" column="ID" >
		<generator class="uuid"/>
	</id>
	<property name="measureId" column="MEASURE_ID"/>
	<property name="userId" column="USER_ID"/>
	<property name="time" column="TIMESTAMP" type="timestamp" />
  </class>
</hibernate-mapping>
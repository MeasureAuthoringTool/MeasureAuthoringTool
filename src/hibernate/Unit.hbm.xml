<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.org/dtd/hibernate-mapping.dtd">
	
<hibernate-mapping auto-import="false">
  <class name="mat.model.Unit" table="UNIT" mutable="false" lazy="false">
	<cache usage="read-only"/>
	<id name="id" column="ID"/>	
	<property name="unit" column="NAME"/>
    <property name="cqlunit" column="CQL_UNIT"/>
	<property name="sortOrder" column="SORT_ORDER" type="integer"/>
  </class>
</hibernate-mapping>
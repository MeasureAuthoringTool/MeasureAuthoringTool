<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
                         " http://castor.codehaus.org/mapping.dtd ">
<mapping>
	<class name="mat.model.DirectReferenceCode">
		<map-to xml="csCode" />

		<field name="codeDescriptor" type="java.lang.String">
			<bind-xml name="codeName" node="element" />
		</field>

		<field name="codeSystemName" type="java.lang.String">
			<bind-xml name="csName" node="element" />
		</field>

		<field name="codeSystemVersion" type="java.lang.String">
			<bind-xml name="csVersion" node="element" />
		</field>
		
		<field name="codeSystemOid" type="java.lang.String">
			<bind-xml name="csOID" node="element" />
		</field>
		
		<field name="code" type="java.lang.String">
			<bind-xml name="code" node="element" />
		</field>
	</class>
</mapping>

<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='login'>

    <inherits name="com.google.gwt.user.User"/>
    <inherits name="com.google.gwt.logging.Logging"/>
    <inherits name='com.google.gwt.user.theme.standard.Standard'/>
    <inherits name="com.google.gwt.xml.XML"/>
    <inherits name='edu.ycp.cs.dh.acegwt.AceGWT'/>
    <!-- Compiling gwt code only for ie7,ie8& firefox 3 & above. -->
    <set-property name="user.agent" value="ie9,ie10,safari,gecko1_8"/>
    <!-- For development, a default of `DEBUG` is recommended
	<inherits name="com.allen_sauer.gwt.log.gwt-log-DEBUG" />
	-->

    <!-- <inherits name='com.google.gwt.user.theme.clean.Clean'/> -->
    <!-- In gwt-log-3.0.2 or earlier releases only, use this instead
     <set-property name="log_RemoteLogger" value="DISABLED" />
    <set-property name="log_WindowLogger" value="DISABLED" />
    <set-property name="log_DivLogger" value="DISABLED" />
    -->
    <inherits name="org.gwtbootstrap3.GwtBootstrap3"/>
    <inherits name="org.gwtbootstrap3.extras.summernote.Summernote"/>
    <inherits name="org.gwtbootstrap3.extras.respond.Respond"/>
    <public path="resource">
        <include name="css/*.*"/>
        <include name="css/*.js"/>
        <include name="fonts/*.*"/>
        <include name="js/*.*"/>
    </public>

    <stylesheet src="css/mat-bootstrap-3.3.2.min.cache.css"/>
    <entry-point class="mat.client.Login"/>
    <source path='client'/>
    <source path='model'/>
    <source path='shared'/>
    <source path='mat.shared.diagramObject'/>
    <source path='dto'/>
    <!-- <add-linker name="xsiframe" /> -->
</module>

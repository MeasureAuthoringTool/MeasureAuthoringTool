<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='login'>
	<inherits name="com.google.gwt.user.User" />
    <inherits name='gwt.g2d.g2d'/>
    <inherits name='org.vaadin.gwtgraphics.GWTGraphics'/>
    <!-- Compiling gwt code only for ie7,ie8& firefox 3 & above. -->
    <set-property name="user.agent" value="ie6,ie8,gecko1_8" />
    
    <!-- For development, a default of `DEBUG` is recommended 
	<inherits name="com.allen_sauer.gwt.log.gwt-log-DEBUG" /> 
	-->
	<!-- In gwt-log-3.0.2 or earlier releases only, use this instead  
	 <set-property name="log_RemoteLogger" value="DISABLED" />
	<set-property name="log_WindowLogger" value="DISABLED" /> 
    <set-property name="log_DivLogger" value="DISABLED" />
    -->
	<entry-point class="org.ifmc.mat.client.Login" />
    <source path='client' />
    <source path='model'/>
    <source path='shared' /> 
    <source path='org.ifmc.mat.shared.diagramObject'/>
    <source path='DTO' />
</module>
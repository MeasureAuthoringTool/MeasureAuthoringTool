<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee webapp_4_0.xsd"
         version="4.0">

    <!-- Default page to serve -->
    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>
            /WEB-INF/applicationContext-service.xml
            /WEB-INF/applicationContext-mail.xml
            /WEB-INF/applicationContext-task.xml
        </param-value>
    </context-param>
    <filter>
        <filter-name>hibernateStatsFilter</filter-name>
        <filter-class>mat.server.HibernateStatisticsFilter</filter-class>
    </filter>
    <filter>
        <filter-name>springSecurityFilterChain</filter-name>
        <filter-class>org.springframework.web.filter.DelegatingFilterProxy</filter-class>
    </filter>
    <filter>
        <filter-name>hibernateFilter</filter-name>
        <filter-class>org.springframework.orm.hibernate5.support.OpenSessionInViewFilter</filter-class>
    </filter>
    <filter>
        <filter-name>PreventCachingFilter</filter-name>
        <filter-class>mat.server.PreventCachingFilter</filter-class>
    </filter>

    <filter-mapping>
        <filter-name>springSecurityFilterChain</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>

    <filter-mapping>
        <filter-name>hibernateStatsFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>hibernateFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>PreventCachingFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>

    <listener>
        <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
    </listener>
    <listener>
        <listener-class>
            org.springframework.security.web.session.HttpSessionEventPublisher
        </listener-class>
    </listener>
    <servlet>
        <servlet-name>adminServlet</servlet-name>
        <servlet-class>mat.server.AdminServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>measureLibraryServlet</servlet-name>
        <servlet-class>mat.server.MeasureServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>featureFlagServlet</servlet-name>
        <servlet-class>mat.server.FeatureFlagRemoteServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CQLLibraryServiceImpl</servlet-name>
        <servlet-class>mat.server.CQLLibraryServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sessionManagementServlet</servlet-name>
        <servlet-class>mat.server.SessionManagementServImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>myAccountServlet</servlet-name>
        <servlet-class>mat.server.MyAccountServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginServiceImpl</servlet-name>
        <servlet-class>mat.server.LoginServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>featureFlagServiceImpl</servlet-name>
        <servlet-class>mat.server.FeatureFlagRemoteServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logoutServiceImpl</servlet-name>
        <servlet-class>mat.server.LoginServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>codelistServiceImpl</servlet-name>
        <servlet-class>mat.server.CodeListServiceImpl</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>packageServiceImpl</servlet-name>
        <servlet-class>mat.server.PackageServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>qDSAttributesServiceImpl</servlet-name>
        <servlet-class>mat.server.clause.QDSAttributesServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>measureCloningServiceImpl</servlet-name>
        <servlet-class>mat.server.clause.MeasureCloningRemoteServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>fhirMeasureRemoteServiceImpl</servlet-name>
        <servlet-class>mat.server.clause.FhirMeasureRemoteServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>exportServlet</servlet-name>
        <servlet-class>mat.server.ExportServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>bulkExportServlet</servlet-name>
        <servlet-class>mat.server.BulkExportServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>fhirValidationReportServlet</servlet-name>
        <servlet-class>mat.server.fhirvalidation.FhirValidationReportServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>auditServiceImpl</servlet-name>
        <servlet-class>mat.server.AuditServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>vsacAPIServiceImpl</servlet-name>
        <servlet-class>mat.server.VSACAPIServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cqlConstantService</servlet-name>
        <servlet-class>mat.server.CQLConstantServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>bonnieService</servlet-name>
        <servlet-class>mat.server.bonnie.BonnieServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>populationService</servlet-name>
        <servlet-class>mat.server.PopulationServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>exportServlet</servlet-name>
        <url-pattern>/mat/export</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>bulkExportServlet</servlet-name>
        <url-pattern>/mat/bulkExport</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fhirValidationReportServlet</servlet-name>
        <url-pattern>/mat/validationReport</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminServlet</servlet-name>
        <url-pattern>/mat/admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sessionManagementServlet</servlet-name>
        <url-pattern>/mat/sessionService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>measureLibraryServlet</servlet-name>
        <url-pattern>/mat/measureLibrary</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>featureFlagServlet</servlet-name>
        <url-pattern>/mat/featureFlag</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>myAccountServlet</servlet-name>
        <url-pattern>/mat/myAccount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginServiceImpl</servlet-name>
        <url-pattern>/login/loginService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logoutServiceImpl</servlet-name>
        <url-pattern>/mat/loginService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logoutServiceImpl</servlet-name>
        <url-pattern>/bonnie/loginService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>codelistServiceImpl</servlet-name>
        <url-pattern>/mat/codelist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cqlConstantService</servlet-name>
        <url-pattern>/mat/cqlconstant</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>bonnieService</servlet-name>
        <url-pattern>/mat/bonnie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>packageServiceImpl</servlet-name>
        <url-pattern>/mat/package</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>qDSAttributesServiceImpl</servlet-name>
        <url-pattern>/mat/qdsattributes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>measureCloningServiceImpl</servlet-name>
        <url-pattern>/mat/measureCloning</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fhirMeasureRemoteServiceImpl</servlet-name>
        <url-pattern>/mat/fhirMeasureService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CQLLibraryServiceImpl</servlet-name>
        <url-pattern>/mat/cqlLibrary</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>auditServiceImpl</servlet-name>
        <url-pattern>/mat/auditService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>vsacAPIServiceImpl</servlet-name>
        <url-pattern>/mat/vsacapi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cqlConstantService</servlet-name>
        <url-pattern>/mat/cqlConstantService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>bonnieService</servlet-name>
        <url-pattern>/mat/bonnieService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>bonnieService</servlet-name>
        <url-pattern>/bonnie/bonnieService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>populationService</servlet-name>
        <url-pattern>/mat/populationService</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>15</session-timeout>
        <!-- To be Commented for Eclipse gwt dev mode on local developer's machine. Never commit the commented cookie-config tag. -->
        <!-- <cookie-config>
           <http-only>true</http-only>
            <secure>true</secure>
        </cookie-config> -->
    </session-config>

</web-app>


